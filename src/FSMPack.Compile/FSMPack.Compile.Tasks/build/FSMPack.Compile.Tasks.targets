<Project>
    <UsingTask TaskName="Tasks.InitializeTask"
        AssemblyFile="$(OutpathPath)FSMPack.Compile.Tasks.dll" />

    <Target Name="MyBeforeTarget" BeforeTargets="Build">
        <InitializeTask />
        <Message Text="Before build target ran" Importance="high" />
    </Target>
    <Target Name="MyAfterTarget" AfterTargets="Build">
        <!-- Run compile on the built dll -->
        <Message Text="After build target ran" Importance="high" />
    </Target>
</Project>
